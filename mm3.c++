#include<iostream>
using namespace std;
int main()
{
	int rows,i,j;
	cout<<"enter no of rows: ";
	cin>>rows;
	for(i=1;i<=rows;i++)
	{
		for(j=1;j<=i;j++)
		{
			cout<<"*";
		}
		cout<<"\n";
}
}
for(i=5;i>=1;i--)
	{
		for(j=i;j>=1;j--)
		{
			cout<<"*";
		}
		cout<<"\n";
	}

